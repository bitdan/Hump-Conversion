{
  "name": "HumpConversion",
  "version": "1.0.0",
  "pluginName": "驼峰转换",
  "description": "驼峰转换插件",
  "main": "index.html",
  "logo": "logo.png",
  "preload": "preload.js",
  "features": [
    {
      "code": "text",
      "cmds": ["驼峰"]
    },
    {
      "code": "regex",
      "cmds": [
        {
          "type": "regex",
          "label": "驼峰",
          "match": "/(.)+/、/[\\s\\S]*/",
          "minLength": 1,
          "maxLength": 10000
        }
      ]
    }
  ]

}
